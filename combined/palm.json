{
  "_defaults": {
    "message_roles": [
      "system",
      "user",
      "assistant"
    ],
    "parameters": [
      {
        "defaultValue": 0.2,
        "key": "temperature",
        "maxValue": 1,
        "minValue": 0
      },
      {
        "defaultValue": 1,
        "key": "top_p",
        "maxValue": 1,
        "minValue": 0
      },
      {
        "defaultValue": 1,
        "key": "candidateCount",
        "maxValue": 8,
        "minValue": 1
      }
    ],
    "pricing_formula": {
      "request": {
        "operands": [
          {
            "value": "input_tokens"
          },
          {
            "value": "rates.request_token"
          }
        ],
        "operation": "multiply"
      },
      "response": {
        "operands": [
          {
            "value": "output_tokens"
          },
          {
            "value": "rates.response_token"
          }
        ],
        "operation": "multiply"
      }
    }
  },
